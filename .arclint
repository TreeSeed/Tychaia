{
  "linters": {
    "csharp": {
      "type": "csharp",
      "include": "(\\.cs$)",
	  "exclude": [ "(\\.Designer\\.cs$)", "(Phabricator\\.Conduit(.*+))" ],
      "binary": "cstools/cslint/bin/Debug/cslint.exe",
      "discovery": {
        "([^/]+)/(.*?)\\.cs": [
          "$1/$1.Linux.csproj"
        ],
        "([^\\\\]+)\\\\(.*?)\\.cs": [
          "$1\\$1.Windows.csproj"
        ]
      }
    },
    "license": {
      "type": "tychaialicense",
      "include": "(\\.cs$)",
	  "exclude": [ "(\\.Designer\\.cs$)", "(Phabricator\\.Conduit(.*+))" ]
    }
  }
}